\n            for value in text_list:\n                if text_list[text_index] == letters_lower[letters_index]:\n                    code_list.append(morse_list[letters_index])\n                    code_list.append(\"/\")\n                    text_index += 1\n                    letters_index = -1\n                letters_index += 1\n            code = convert_to_text(code_list)\n            basic.show_string(code)\n            index = 0\n            while index <= len(code) - 1:\n                if code.char_at(index) == \".\":\n                    P0_servo_movement()\n                elif code.char_at(index) == \"-\":\n                    P1_servo_movement()\n                else:\n                    Both_servo_movement()\n                basic.pause(100)\n                index += 1\n        else:\n            basic.pause(3000)\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"eye_switcher\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n"}},{"timestamp":1765569053368,"editorVersion":"8.0.18","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"e}hj.3N!L*aaF9OLOBUD\">Servo_up</variable><variable id=\"W~|4t!whrh@*c-+Y=`wA\">close_duration</variable><variable id=\"ApDZ#y4fi.#cy8Rh;*qu\">mode</variable><variable id=\"LL7xvDw[FDe-ravuO:)x\">word</variable><variable id=\"{M4JLELHEQ$I=]VgO[G3\">text_list</variable><variable id=\"d/R9C5+Wy44O?is:y1`n\">index1</variable><variable id=\"lZ}O`GE!|@1CTT6Gt]_2\">text_index</variable><variable id=\"3`kUI98AN-EQyMT@|6CL\">letters_index</variable><variable id=\"3XYqlGi{}p0$RB{:ZW0p\">letters_lower</variable><variable id=\"Kn`0etwJh*4xjw^4nhgD\">morse_list</variable><variable id=\"SMq7DDqzVaSi2{%4*{R;\">code<